{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate:types": "openapi-typescript ../dist/openapi.json -o src/api/types.ts",
    "smoke": "node smoke.mjs",
    "smoke:clarify": "node clarify-smoke.mjs",
    "smoke:clarify-export": "node clarify-export-smoke.mjs",
    "smoke:clarify-all": "node clarify-smoke-combined.mjs",
    "test": "vitest run",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "ci": "npm run lint && npm run format:check && npm run smoke"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.2.5",
    "eslint": "^9.13.0",
    "@eslint/js": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.8.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "ws": "^8.18.0",
    "vitest": "^1.6.0"
  }
}
